    "scripts": {
        "start": "react-scripts start",
        "start:circles_staging": "env-cmd -f .env.circles_staging npm start",
        "start:circles_prod": "env-cmd -f .env.circles_prod npm start",
        "start:circles_devprod": "env-cmd -f .env.circles_devprod npm start",
        "start:codo_prod": "env-cmd -f .env.codo_prod npm start",
        "start:codo_devprod": "env-cmd -f .env.codo_devprod npm start",
        "build": "react-scripts build && copy build\\index.html functions\\hosting\\index.html && del build\\index.html",
        "build:circles_staging": "env-cmd -f .env.circles_staging npm run build",
        "build:circles_prod": "env-cmd -f .env.circles_prod npm run build",
        "build:codo_prod": "env-cmd -f .env.codo_prod npm run build",
        "deploy:circles_staging": "npm run build:circles_staging && firebase deploy -P circles_staging",
        "deploy:circles_prod": "npm run build:circles_prod && firebase deploy -P circles_prod",
        "deploy:codo_prod": "npm run build:codo_prod && firebase deploy -P codo_prod",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "config": {
        "REACT_APP_ENV": ".env"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not ie 11",
            "not chrome < 51",
            "not safari < 10"
